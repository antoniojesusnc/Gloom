{\r
	Rotation = rand(0, 2*pi);\r
	Position = float3(0,0.4,0);\r
	Velocity = Velocity * 1.2 * ((1.0 - pow(rand(0, 1), 3)) * 8 + 4.0) * float3(1,0,1) + float3(0,1,0);\r
