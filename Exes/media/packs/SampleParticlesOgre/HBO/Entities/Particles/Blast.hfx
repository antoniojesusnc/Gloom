	Expression = "// TrailSmoke: Spawn\r
function void	Eval()\r
{\r
	Position = float3(0);\r
	Rotation = rand(0,2*pi);\r
	Velocity = float3(0,0,2) + 0.3 * (float3(1,0,0) * sin(SpawnerLifeRatio * 169.0) + float3(0,1,0) * sin(SpawnerLifeRatio * 169.0 + pi*0.5));\r
\r
	float	spawnerDecay = 1.0 - SpawnerLifeRatio;\r
\r
	Life = 0.01 + rand(4.3,4.5) * pow(spawnerDecay, 2.0);\r
	Brightness = 1.0 - rand(0.4,0.6) * pow(spawnerDecay, 2.0);\r
\r
	float	trailStrength = pow(spawnerDecay, 8.0);\r
	AlphaCoeff = trailStrength;\r
	SizeCoeff = 0.6 + trailStrength * (1.5 + sin(SpawnerLifeRatio * 350.0) * 0.2);\r
	TextureID = rand(0,4);\r
}";
}
