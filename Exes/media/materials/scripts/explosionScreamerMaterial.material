
material explosionScreamerMaterial
{
	technique
	{

		pass
		{
			depth_check off

			texture_unit 0
			{
                tex_coord_set 0
				tex_address_mode clamp
				filtering linear linear linear
			}

			texture_unit 1
			{
				texture ExplosionScreamer2.png 2d
                tex_coord_set 1
				tex_address_mode clamp
				filtering linear linear linear
			}

		}
	}
}
