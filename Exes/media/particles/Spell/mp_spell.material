//-----------------------------------------------------------------------------------------------------------------------
fragment_program mp_fp_spell_sphere hlsl
{
	source mp_spell_sphere_sm20.hlsl
	target ps_2_0
	entry_point mainFP
}

material mp_spell_sphere
{
    technique
    {
        pass
        {
            lighting off
            depth_write off
            scene_blend add

            fragment_program_ref mp_fp_spell_sphere
            {
                param_named_auto time time_0_x 6.28
            }

            texture_unit
            {
                texture mp_spell_outer.dds
            }
        }
    }
	
	technique {
		lighting off
		
		scheme depthScheme
		pass {
			vertex_program_ref depthMapVertex {
			}
			
			fragment_program_ref depthMapFragment {
			}
		}
	}
	
	technique {
		lighting off
	
		scheme blackScheme
		pass {
			vertex_program_ref blackVertex {
			}
			
			fragment_program_ref blackFragment {
			}
		}
	}
}
